{
  "name": "@VizMediaEurope/adn-core",
  "version": "3.0.1",
  "description": "core package for adn microservices",
  "repository": "git@gitlab.com:VizMediaEurope/adn-core.git",
  "author": "adn-team",
  "license": "ISC",
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "husky": {
    "hooks": {
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "yarn typecheck",
      "yarn lint:fix",
      "yarn lint",
      "yarn test"
    ]
  },
  "scripts": {
    "build": "rm -rf ./lib && yarn tsc -p . && yarn copy-static",
    "dev-build": "sh ./dev-build.sh",
    "test": "yarn jest",
    "lint": "yarn eslint --ext .ts src/",
    "lint:fix": "yarn lint --fix",
    "prepare": "yarn build",
    "copy-static": "yarn copyfiles './src/static/**' './lib'",
    "typecheck": "yarn tsc --project tsconfig.json --noEmit --pretty false"
  },
  "dependencies": {
    "@hapi/joi": "17.1.1",
    "@sentry/node": "5.11.2",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.6",
    "@types/express-pino-logger": "4.0.2",
    "@types/hapi__joi": "16.0.9",
    "@types/i18n": "0.8.6",
    "@types/jest": "25.1.2",
    "@types/jsonwebtoken": "8.3.8",
    "@types/md5": "2.2.0",
    "@types/node": "13.5.3",
    "@types/node-mailjet": "3.3.3",
    "@types/pino": "6.0.1",
    "@types/ramda": "0.27.4",
    "@types/supertest": "2.0.8",
    "@types/tsscmp": "1.0.0",
    "@typescript-eslint/eslint-plugin": "2.18.0",
    "@typescript-eslint/parser": "2.18.0",
    "axios": "0.19.2",
    "cookie-parser": "1.4.5",
    "copyfiles": "2.2.0",
    "cors": "2.8.5",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-prettier": "3.1.2",
    "express": "4.17.1",
    "express-pino-logger": "4.0.0",
    "global": "^4.4.0",
    "handlebars": "4.7.3",
    "husky": "4.2.1",
    "i18n": "0.8.4",
    "jest": "25.1.0",
    "jsonwebtoken": "8.5.1",
    "md5": "2.2.1",
    "mocked-env": "1.3.2",
    "mysql": "2.17.1",
    "node-mailjet": "3.3.1",
    "pino": "5.16.0",
    "prettier": "1.19.1",
    "ramda": "0.27.0",
    "supertest": "4.0.2",
    "ts-jest": "25.1.0",
    "ts-node": "8.6.2",
    "tsscmp": "1.0.6",
    "typescript": "^3.9.5"
  },
  "publishConfig": {
    "@VizMediaEurope:registry": "https://gitlab.com/api/v4/projects/16836221/packages/npm/"
  },
  "devDependencies": {}
}
